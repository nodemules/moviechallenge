main-search
.typeahead-search(ng-controller='typeaheadController')
	input(
		type='text'
		ng-model='searchText'
		placeholder='Typeahead test'
		class='form-control'
		)
	ul(id='suggestionList')
		li(ng-repeat="movie in movies" ng-click="$parent.searchText = movie.title") {{ movie.title }}
//
	input(
		type='text'
		ng-model='searchText'
		typeahead='a for a in getMovieTypeahead($viewValue)'
		class='form-control'
		)